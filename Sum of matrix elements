#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

void sum(){
         int n,a;
            cin>>n;
        int b=0;
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                int x;
                cin>>x;
                if(i==j || i==n-1 || j==0 || i==0 || j==(n-1) || (i+j)==(n-1))
                 b=b+x;
                
            }
        }
        
        cout<<b<<endl;
        
}


int main() {
    int t;
    cin>>t;
    while(t--){
        sum();
    }
    
    return 0;
}
